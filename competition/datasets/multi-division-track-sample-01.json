  "constraints": [
    {
      "constraintId": "no_overlap_team",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "no_overlap_venue",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "min_rest_time",
      "type": "hard",
      "params": {
        "min_hours": 72
      }
    },
    {
      "constraintId": "venue_availability",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "max_consecutive_away",
      "type": "hard",
      "params": {
        "max_consecutive": 2
      }
    },
    {
      "constraintId": "season_bounds",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "division_specific_rest",
      "type": "hard",
      "params": {
        "selectorV2": {
          "when": {
            "team.tier": {"equals": 1}
          },
          "apply": {
            "min_hours": 96
          }
        }
      },
      "description": "Premier division teams require 96 hours rest instead of standard 72 hours"
    },
    {
      "constraintId": "shared_venue_separation",
      "type": "hard",
      "params": {
        "selectorV2": {
          "when": {
            "venue.sharedBy": {"count": {"greaterThan": 1}}
          },
          "apply": {
            "min_separation_hours": 24
          }
        }
      },
      "description": "Shared venues must have 24 hours between fixtures"
    },
    {
      "constraintId": "cup_fixture_priority",
      "type": "soft",
      "weight": 8,
      "params": {
        "selectorV2": {
          "when": {
            "fixture.fixtureType": {"equals": "cup"}
          },
          "apply": {
            "preferredSlots": [
              {"dayOfWeek": "Wednesday", "startTime": "19:45", "endTime": "21:45"}
            ]
          }
        }
      },
      "description": "Cup fixtures should be scheduled on Wednesday evenings when possible"
    },
    {
      "constraintId": "international_travel_buffer",
      "type": "soft",
      "weight": 7,
      "params": {
        "selectorV2": {
          "when": {
            "travel.crosses_timezone": true
          },
          "apply": {
            "min_buffer_hours": 48
          }
        }
      },
      "description": "International travel should include 48-hour buffer when crossing time zones"
    },
    {
      "constraintId": "promotion_zone_fairness",
      "type": "soft",
      "weight": 9,
      "params": {
        "selectorV2": {
          "when": {
            "fixture.homeTeam.tier": {"in": [2, 3, 4]},
            "fixture.awayTeam.tier": {"in": [2, 3, 4]}
          },
          "apply": {
            "balanceHomeAwaySequence": true,
            "maxConsecutiveSameSide": 2
          }
        }
      },
      "description": "Lower division teams in promotion contention need balanced home/away schedules"
    }
  ],
  "objectives": [
    {
      "objectiveId": "minimize_total_travel",
      "weight": 5,
      "params": {}
    },
    {
      "objectiveId": "balance_home_away",
      "weight": 4,
      "params": {
        "maxDeviation": 1
      }
    },
    {
      "objectiveId": "tier_based_broadcast_priority",
      "weight": 6,
      "params": {
        "tierWeights": {
          "1": 3.0,
          "2": 2.0,
          "3": 1.5,
          "4": 1.0
        },
        "primeTimeSlots": {
          "Saturday_15:00-17:00": 1.0,
          "Saturday_17:30-20:00": 1.5,
          "Saturday_20:00-23:00": 1.8,
          "Sunday_16:00-20:00": 1.4,
          "Wednesday_19:45-22:00": 1.3
        }
      },
      "description": "Higher tier divisions get priority for premium broadcast windows"
    },
    {
      "objectiveId": "cross_division_excitement",
      "weight": 4,
      "params": {
        "spreadCupFixtures": true,
        "avoidClumping": true
      },
      "description": "Distribute cup fixtures (cross-division) throughout the season"
    },
    {
      "objectiveId": "shared_venue_optimization",
      "weight": 5,
      "params": {
        "minimizeConflicts": true,
        "preferAlternatingUse": true
      },
      "description": "Optimize schedules for teams sharing venues to minimize conflicts and maximize facility utilization"
    },
    {
      "objectiveId": "international_travel_clustering",
      "weight": 3,
      "params": {
        "clusterInternationalGames": true,
        "maxClusterSize": 3
      },
      "description": "When teams travel internationally, cluster multiple away games to reduce total travel"
    }
  ]
