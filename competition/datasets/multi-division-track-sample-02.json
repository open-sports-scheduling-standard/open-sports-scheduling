  "constraints": [
    {
      "constraintId": "no_overlap_team",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "no_overlap_venue",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "min_rest_time",
      "type": "hard",
      "params": {
        "min_hours": 48
      }
    },
    {
      "constraintId": "venue_availability",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "season_bounds",
      "type": "hard",
      "params": {}
    },
    {
      "constraintId": "parent_club_priority",
      "type": "hard",
      "params": {
        "selectorV2": {
          "when": {
            "team.metadata.parentNHL": {"exists": true},
            "venue.sharedBy": {"contains": ["nhl", "ahl"]}
          },
          "apply": {
            "blockAHLWhenNHLHome": true,
            "minSeparationHours": 24
          }
        }
      },
      "description": "AHL teams sharing arena with NHL parent cannot play home games when NHL team has home game within 24 hours"
    },
    {
      "constraintId": "cross_border_buffer",
      "type": "hard",
      "params": {
        "selectorV2": {
          "when": {
            "travel.crossesBorder": true
          },
          "apply": {
            "minBufferHours": 12
          }
        }
      },
      "description": "Teams crossing US/Canada border need 12-hour buffer for customs clearance"
    },
    {
      "constraintId": "tv_broadcast_deconfliction",
      "type": "soft",
      "weight": 9,
      "params": {
        "selectorV2": {
          "when": {
            "team.tier": 1,
            "fixture.dayOfWeek": {"in": ["Wednesday", "Saturday"]}
          },
          "apply": {
            "maxSimultaneousGames": 2,
            "staggerStartTimes": 30
          }
        }
      },
      "description": "NHL games on national TV nights should be staggered to avoid broadcast conflicts"
    },
    {
      "constraintId": "call_up_buffer",
      "type": "soft",
      "weight": 7,
      "params": {
        "selectorV2": {
          "when": {
            "team.metadata.parentNHL": {"exists": true}
          },
          "apply": {
            "avoidBackToBackBeforeNHLHome": true
          }
        }
      },
      "description": "AHL teams should avoid back-to-back games before NHL parent home games to allow emergency call-ups"
    },
    {
      "constraintId": "isolated_team_clustering",
      "type": "soft",
      "weight": 8,
      "params": {
        "selectorV2": {
          "when": {
            "team.metadata.isolated": true
          },
          "apply": {
            "clusterHomeGames": true,
            "minHomestand": 3,
            "maxHomestand": 6
          }
        }
      },
      "description": "Geographically isolated teams (e.g., Newfoundland) should have clustered home/road segments"
    }
  ],
  "objectives": [
    {
      "objectiveId": "minimize_total_travel",
      "weight": 6,
      "params": {}
    },
    {
      "objectiveId": "balance_home_away",
      "weight": 4,
      "params": {
        "maxDeviation": 2
      }
    },
    {
      "objectiveId": "shared_venue_efficiency",
      "weight": 7,
      "params": {
        "maximizeUtilization": true,
        "minimizeConflicts": true
      },
      "description": "Optimize shared arena usage between NHL/AHL teams"
    },
    {
      "objectiveId": "tier_competitive_balance",
      "weight": 5,
      "params": {
        "balanceWithinTier": true,
        "avoidWeakStartOrEnd": true
      },
      "description": "Ensure fair strength of schedule within each tier"
    },
    {
      "objectiveId": "minimize_border_crossings",
      "weight": 6,
      "params": {
        "clusterInternationalTrips": true
      },
      "description": "Cluster cross-border travel to minimize customs/immigration processing"
    }
  ]
