      "params": {"maxImbalance": 1},
      "penalty": 800,
      "type": "soft"
    },
    {
      "id": "weekend-games-preferred",
      "rule": "time_window",
      "params": {
        "preferredDays": ["friday", "saturday", "sunday"],
        "percentageRequired": 60
      },
      "penalty": 100,
      "type": "soft"
    },
    {
      "id": "no-monday-games",
      "rule": "time_window",
      "params": {"excludedDays": ["monday"]},
      "penalty": 300,
      "type": "soft"
    },
    {
      "id": "derby-separate",
      "rule": "buffer_time",
      "params": {
        "teams": [["team-1", "team-2"], ["team-3", "team-4"], ["team-5", "team-6"]],
        "minDaysBetween": 14
      },
      "penalty": 400,
      "type": "soft",
      "description": "Teams sharing venues should not play home games within 2 weeks of each other when possible"
    },
    {
      "id": "round-spread",
      "rule": "round_distribution",
      "params": {
        "minDaysBetweenRounds": 4,
        "maxDaysBetweenRounds": 8
      },
      "penalty": 200,
      "type": "soft"
    },
    {
      "id": "primetime-conflict-avoidance",
      "rule": "no_simultaneous_group_games",
      "params": {
        "sameSlotLimit": 1,
        "slots": ["19:00", "19:30", "20:00"]
      },
      "penalty": 250,
      "type": "soft",
      "description": "Avoid multiple games at same primetime to reduce broadcast conflicts"
    },
    {
      "id": "turnaround-enforcement",
      "rule": "buffer_time",
      "params": {
        "scope": "venue",
        "useVenueTurnaround": true
      },
      "penalty": 5000,
      "type": "hard"
    }
  ],
